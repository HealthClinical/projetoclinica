class Paciente:
	def  __init__(self,codPaciente, nome, rua, numero, bairro, cidade, uf, telefone, cpf, rg, dataNasc, convenio):

		self.codPaciente = codPaciente
		self.nome = nome
		self.rua= rua
		self.numero = numero
		self.bairro = bairro
		self.cidade = cidade
		self.uf = uf
		self.telefone = telefone
		self.cpf = cpf
		self.rg = rg
		self.dataNasc = dataNasc
		self.convenio = convenio
		








			

		
